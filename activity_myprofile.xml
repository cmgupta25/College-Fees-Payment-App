<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBack"
    tools:context="com.project.esh_an.vemanafeeportal.myprofile">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout"
        android:layout_marginTop="220sp"
        android:background="@drawable/round_window_down">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView9"
            android:layout_marginTop="20dp"
            android:text="Name"
            android:textColor="#ffff"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView10"
            android:layout_below="@+id/textView8"
            android:textColor="#ffff"
            android:layout_marginTop="25dp"
            android:text="USN"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView5"
            android:layout_below="@+id/textView9"
            android:layout_marginTop="25dp"
            android:text="Email-id"
            android:textColor="#ffff"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView10"
            android:layout_marginStart="25dp"
            android:layout_marginTop="25dp"
            android:text="Branch"
            android:textColor="#ffff"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView5"
            android:layout_alignStart="@+id/textView9"
            android:layout_marginTop="25dp"
            android:text="Year"
            android:textColor="#ffff"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/name_myprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView8"
            android:layout_alignBottom="@+id/textView8"
            android:layout_marginStart="140dp"
            android:hint="Name"
            android:textSize="20sp"
            android:textColor="#000"/>

        <TextView
            android:id="@+id/usn_myprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/name_myprofile"
            android:textColor="#000"
            android:layout_alignBaseline="@+id/textView9"
            android:layout_below="@+id/textView8"
            android:layout_marginTop="25dp"
            android:hint="USN"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/email_myprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/usn_myprofile"
            android:layout_below="@+id/textView9"
            android:layout_marginTop="25dp"
            android:layout_alignBaseline="@+id/textView10"
            android:hint="Email-id"
            android:textColor="#000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/branch_myprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView10"
            android:layout_alignStart="@+id/email_myprofile"
            android:layout_marginTop="25dp"
            android:hint="Branch"
            android:layout_alignBaseline="@+id/textView5"
            android:textColor="#000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/year_myprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView5"
            android:layout_alignStart="@+id/branch_myprofile"
            android:layout_marginTop="25dp"
            android:layout_alignBaseline="@+id/textView11"
            android:hint="Year"
            android:textColor="#000"
            android:textSize="20sp" />

    </RelativeLayout>
    <ImageView
        android:background="@drawable/shadow"
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="217dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="2dp"
        app:srcCompat="@drawable/profile"
        android:onClick="onClick"
        tools:ignore="OnClick" />

    <ImageButton
        android:id="@+id/upload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentEnd="true"
        app:srcCompat="@drawable/ic_visibility_grey_900_24dp" />

</RelativeLayout>
